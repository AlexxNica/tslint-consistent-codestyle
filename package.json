{
  "name": "tslint-consistent-codestyle",
  "version": "0.1.2",
  "description": "Enforce constistent code style with tslint",
  "repository": {
    "type": "git",
    "url": "https://github.com/ajafff/tslint-consistent-codestyle"
  },
  "scripts": {
    "test": "echo test/rules/*/* | xargs -n 1 tslint --test",
    "pretest": "npm run build",
    "prebuild": "npm run lint",
    "build": "tsc -p .",
    "prepublish": "npm test",
    "lint": "tslint '{rules,src}/**/*.ts'"
  },
  "keywords": [
    "eslint",
    "tslint",
    "custom-rules",
    "rules",
    "stylish",
    "lint",
    "linting",
    "linter",
    "tslint-plugin"
  ],
  "license": "MIT",
  "dependencies": {
    "tslint": "^4.0.2"
  },
  "devDependencies": {
    "typescript": "^2.1.4"
  }
}
