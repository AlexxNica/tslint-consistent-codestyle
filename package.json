{
  "name": "tslint-consistent-codestyle",
  "version": "1.1.0",
  "description": "Enforce constistent code style with tslint",
  "repository": {
    "type": "git",
    "url": "https://github.com/ajafff/tslint-consistent-codestyle"
  },
  "main": "rules/index.js",
  "scripts": {
    "test": "echo test/rules/*/* | xargs -n 1 tslint --test",
    "build": "tsc -p .",
    "prepublish": "npm run verify",
    "lint": "tslint '{rules,src}/**/*.ts'",
    "lint:fix": "tslint --fix '{rules,src}/**/*.ts'",
    "verify": "npm run build && npm run lint && npm run coverage",
    "coverage": "nyc npm test"
  },
  "keywords": [
    "eslint",
    "tslint",
    "custom-rules",
    "rules",
    "stylish",
    "lint",
    "linting",
    "linter",
    "tslint-plugin"
  ],
  "license": "MIT",
  "dependencies": {
    "tsutils": "^1.4.0"
  },
  "devDependencies": {
    "nyc": "^10.2.0",
    "tslint": "^5.0.0",
    "typescript": "^2.1.4"
  },
  "peerDependencies": {
    "tslint": "^5.0.0",
    "typescript": "^2.1.4 || >=2.1.0-dev || >=2.2.0-dev || >=2.3.0-dev"
  }
}
